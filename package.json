{
  "name": "express-ts-boilerplate",
  "version": "0.0.1",
  "description": "A TS express boilerplate using typedi and routing-controllers.",
  "author": "hongxin.tang@hotmail.com",
  "homepage": "https://github.com/tanghongxin/express-ts-boilerplate",
  "license": "MIT",
  "main": "bundle/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "prd": "cross-env NODE_ENV=production nodemon",
    "build": "rimraf bundle && cross-env NODE_ENV=production tsc && tsc-alias",
    "test": "test",
    "lint": "eslint --ext .js,.ts ./src",
    "lint:fix": "eslint --ext .js,.ts ./src --fix"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "keywords": [
    "express",
    "routing-controllers",
    "typedi",
    "MVC"
  ],
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "express": "^4.16.1",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "log4js": "^6.9.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.4",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "typedi": "^0.10.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.7",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.5.3"
  }
}
